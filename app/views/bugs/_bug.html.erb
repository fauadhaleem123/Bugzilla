    <p>
        <strong>Bug Title: </strong>
        <%= bug.title %>
    </p>
    <p>
        <strong>Deadline: </strong>
        <%= bug.deadline %>
    </p>
    <p>
        <strong>Screenshot: </strong>
        <%= image_tag bug.image_url(:thumb).to_s %>
    </p>
    <p>
        <strong>Type: </strong>
        <%= bug.bug_type %>
    </p>
    <p>
        <strong>status: </strong>
        <%= bug.status %>
    </p>
    <% if current_user.role =="Developer" && current_user.bug_id != bug.id %>
        <%= link_to "Assign Bug", assign_bug_project_bug_path(:user_id => current_user.id, :bug_id => bug.id) %>
    <% end %>
